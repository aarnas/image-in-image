{
  "name": "image-in-image",
  "version": "1.0.0",
  "description": "Package for finding image as a part of another image and return its position.",
  "main": "index.js",
  "scripts": {
    "dev": "npx nodemon",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/index.js",
    "test": "npm run build && npm run copy-test-files && node build/test/test.js",
    "copy-test-files": "cp ./src/test/*.png ./build/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarnas/image-in-image.git"
  },
  "keywords": [
    "find",
    "image",
    "in",
    "image",
    "position",
    "cordinates"
  ],
  "author": "aarnas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aarnas/image-in-image/issues"
  },
  "homepage": "https://github.com/aarnas/image-in-image#readme",
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/sharp": "^0.31.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "jimp": "^0.16.2",
    "sharp": "^0.31.2"
  }
}
